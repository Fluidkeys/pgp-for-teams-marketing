<script>
    var icons = [
      "|","/","-","\\"
    ]

    var spinners = document.querySelectorAll('.spinner');

    function startSpinning(icons,spinner) {
        var i = 0;
        setInterval(function() {
            spinner.innerHTML = icons[i];
            i = (i + 1) % icons.length;
        }, 100);
    };

    startSpinning(icons,spinners[0]);

</script>
